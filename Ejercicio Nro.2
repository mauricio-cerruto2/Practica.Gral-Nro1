package Ejercicios;

public class Ejercicio2 {
	    private String marca;
	    private String modelo;
	    private int velocidad;

	    public Ejercicio2(String marca, String modelo) {
	        this.marca = marca;
	        this.modelo = modelo;
	        this.velocidad = 0;
	    }

	    public void acelerar() {
	        velocidad += 10;
	    }

	    public void frenar() {
	        velocidad -= 5;
	        if (velocidad < 0) {
	            velocidad = 0;
	        }
	    }

	    public int getVelocidad() {
	        return velocidad;
	    }

	    public static void main(String[] args) {
	        
	    	Ejercicio2 coche1 = new Ejercicio2("Toyota", "Corolla");
	    	Ejercicio2 coche2 = new Ejercicio2("Ford", "Mustang");

	       
	        coche1.acelerar();
	        coche1.acelerar();
	        coche1.frenar();

	        coche2.acelerar();
	        coche2.frenar();
	        coche2.acelerar();
	        coche2.acelerar();
	        coche2.frenar();

	        
	        System.out.println("Velocidad del " + coche1.marca + " " + coche1.modelo + ": " + coche1.getVelocidad() + " km/h");
	        System.out.println("Velocidad del " + coche2.marca + " " + coche2.modelo + ": " + coche2.getVelocidad() + " km/h");
	    }
	

}


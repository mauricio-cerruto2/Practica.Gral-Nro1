class Coche:
    def _init_(self, marca, modelo):
        self.marca = marca
        self.modelo = modelo
        self.velocidad = 0
    
    def acelerar(self):
        self.velocidad += 10
        return self.velocidad
    
    def frenar(self):
        self.velocidad = max(0, self.velocidad - 5)  
        return self.velocidad
    
    def mostrar_velocidad(self):
        return f"{self.marca} {self.modelo}: Velocidad actual {self.velocidad} km/h"


